from re import I
import sys
is_raining = input("is it raining? yes or no: ")
has_raincaot = input("do you even have a raincoat? yes or no: ")

is_raining = is_raining == "yes"
has_raincaot = has_raincaot == "yes"

if is_raining:
    if has_raincaot:
        print("raincoat")
    else:
        print("buy a raincoat")
else:
    print("no raincoat")

